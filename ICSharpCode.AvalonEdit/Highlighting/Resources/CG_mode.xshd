  <RuleSet name="CharactersAndEscapeCharacters">
    <Span begin="\\" end="." />
  </RuleSet>


  <RuleSet name="PreprocessorSet">
    <Span>
      <!-- preprocessor directives that allow other spans -->
      <Begin>
        (define|undef|include|if|ifdef|ifndef|elif|else|endif|line)\b
      </Begin>
      <RuleSet>
        <Span color="LocalIncludeFile">
          <Begin>"</Begin>
          <End>"</End>
        </Span>
        <Span color="GlobalIncludeFile">
          <Begin>&lt;</Begin>
          <End>&gt;</End>
        </Span>
        <Span color="Comment" ruleSet="CommentMarkerSet">
          <Begin>//</Begin>
        </Span>
      </RuleSet>
    </Span>
    <Span>
      <!-- preprocessor directives that don't allow other spans -->
      <Begin>
        (error|pragma)\b
      </Begin>
    </Span>
  </RuleSet>


  <RuleSet name="CommentMarkerSet">
    <Keywords fontWeight="bold" foreground="DarkRed">
      <Word>TODO</Word>
      <Word>FIXME</Word>
    </Keywords>
    <Keywords fontWeight="bold" foreground="DarkRed">
      <Word>HACK</Word>
      <Word>UNDONE</Word>
    </Keywords>
  </RuleSet>


  <!-- Rules shared between default rule set, AssemblerSet, and TechniquesAndPassesSet. -->
  <RuleSet name="CommonSet">
    <Rule color="Punctuation">
      [?,.;()\[\]{}+\-/%*&lt;&gt;^=~!|&amp;]+
    </Rule>

    <Span color="Preprocessor" ruleSet="PreprocessorSet">
      <Begin>\#</Begin>
    </Span>

    <Span color="Comment" ruleSet="CommentMarkerSet">
      <Begin>//</Begin>
    </Span>

    <Span color="Comment" ruleSet="CommentMarkerSet" multiline="true">
      <Begin>/\*</Begin>
      <End>\*/</End>
    </Span>

    <Span color="String" ruleSet="CharactersAndEscapeCharacters">
      <Begin>"</Begin>
      <End>"</End>
    </Span>

    <Span color="Char" ruleSet="CharactersAndEscapeCharacters">
      <Begin>'</Begin>
      <End>'</End>
    </Span>

    <Rule color="NumberLiteral">
      (?i)                                            # Ignore case
      \b0x[0-9A-F]+(lu|ul|l|u)?                       # Hex number
      |(\b\d+(\.\d+)?|\.\d+)([e][+-]?\d+)?(h|f|d|l)?  # Decimal number
      |\b\d+(?!\.)(lu|ul|l|u|h|f|d)?                  # Integer number
    </Rule>

