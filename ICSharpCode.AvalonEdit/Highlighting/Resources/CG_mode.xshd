  <Color name="Char" foreground="DarkRed" exampleText="char linefeed = '\n';"/>
  <Color name="Punctuation" exampleText="a(b.c);" />
  <Color name="MethodCall" foreground="MidnightBlue" fontWeight="bold" exampleText="float3 v = mul(p, m);"/>
  <Color name="NumberLiteral" foreground="DarkRed" exampleText="3.1415f"/>
  <Color name="Literals" foreground="Blue"/>

  <Color name="Preprocessor" foreground="Blue" exampleText="#error Message" />
  <Color name="LocalIncludeFile" foreground="DarkRed" exampleText="#include &quot;common.fxh&quot;"/>
  <Color name="GlobalIncludeFile" foreground="DarkRed" exampleText="#include &lt;common.fxh&gt;"/>

