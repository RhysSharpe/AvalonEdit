  <RuleSet name="TechniquesAndPassesSet">
    <Import ruleSet="CommonSet"/>
    <Import ruleSet="NonAssemblerSet"/>

    <Span ruleSet="TechniquesAndPassesSet" multiline="true">
      <Begin color="Pass">
        \b(pass|Pass)\b
      </Begin>
      <End>}</End>
    </Span>

    <Span ruleSet="TechniquesAndPassesSet" multiline="true">
      <Begin color="StateBlock">
        \b(sampler_state|stateblock_state)\b
      </Begin>
      <End>}</End>
    </Span>

    <Span ruleSet="AssemblerSet" multiline="true">
      <Begin color="Assembler">\basm\b</Begin>
      <End>}</End>
    </Span>

    <Rule color="TechniqueName">
      (?&lt;=\b(technique|Technique)\s*)
      [\d\w_]+
    </Rule>

    <Rule color="PassName">
      (?&lt;=\b(pass|Pass)\s*)
      [\d\w_]+
    </Rule>

    <!-- 
        Some effect files (mainly CgFX files) sometimes use array initializers for effect states, e.g. "ColorMask = { 1, 1, 1, 1 };"
        But we assume that a '}' closes the current pass.
        To ignore such initializers, we add the following rule.
      -->
    <Span ruleSet="TechniquesAndPassesSet" multiline="true">
      <Begin>=\s*{</Begin>
      <End>}</End>
    </Span>

